{
    "agent": {
      "metrics_collection_interval": 10,
      "logfile": "/opt/aws/amazon-cloudwatch-agent/logs/amazon-cloudwatch-agent.log"
    },
    "metrics": {
      "metrics_collected": {
          "procstat": [
              {
                  "pid_file": "/var/run/jenkins/jenkins.pid",
                  "measurement": [
                      "pid_count"
                  ]
              }
              ]
        }
    },
    "logs": {
      "logs_collected": {
        "files": {
          "collect_list": [
            {
              "file_path": "/var/log/jenkins/jenkins.log",
              "log_group_name": "/var/log/jenkinslogs",
              "log_stream_name": "jenkins.log.{ip_address}",
              "timezone": "UTC"
            }
          ]
        }
      },
      "log_stream_name": "jenkins_master_log",
      "force_flush_interval" : 30
    }
}